{
  "name": "thryfted-mobile",
  "version": "1.0.0",
  "description": "Thryfted marketplace mobile app built with React Native",
  "main": "index.js",
  "scripts": {
    "start": "npx react-native start",
    "ios": "npx react-native run-ios",
    "android": "npx react-native run-android",
    "build:ios": "cd ios && xcodebuild -workspace ThroftedMobile.xcworkspace -scheme ThroftedMobile archive",
    "build:android": "cd android && ./gradlew assembleRelease",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "clean": "npx react-native clean",
    "pods": "cd ios && pod install",
    "dev:reset-cache": "npx react-native start --reset-cache"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.6.2",
    "@reduxjs/toolkit": "^2.0.1",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-image-picker": "^7.1.0",
    "react-native-image-crop-picker": "^0.40.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal": "^13.0.1",
    "react-native-toast-message": "^2.2.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-device-info": "^10.13.0",
    "react-native-permissions": "^4.1.1",
    "react-native-contacts": "^7.0.8",
    "react-native-share": "^10.0.2",
    "react-native-config": "^1.5.1",
    "react-native-keychain": "^8.2.0",
    "react-native-biometrics": "^3.0.1",
    "@react-native-camera-roll/camera-roll": "^7.4.0",
    "react-native-maps": "^1.10.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-push-notification": "^8.1.1",
    "@react-native-firebase/app": "^18.8.0",
    "@react-native-firebase/messaging": "^18.8.0",
    "@react-native-firebase/analytics": "^18.8.0",
    "@react-native-firebase/crashlytics": "^18.8.0",
    "react-native-flipper": "^0.212.0",
    "flipper-plugin-react-native-performance": "^1.0.1",
    "axios": "^1.6.5",
    "react-hook-form": "^7.49.2",
    "@hookform/resolvers": "^3.3.2",
    "yup": "^1.4.0",
    "date-fns": "^3.3.1",
    "lodash": "^4.17.21",
    "react-native-dotenv": "^3.4.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/lodash": "^4.14.202",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4",
    "@testing-library/react-native": "^12.4.3",
    "@testing-library/jest-native": "^5.4.3",
    "detox": "^20.14.8",
    "flipper": "^0.212.0"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/*.(test|spec).(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "private": true
}